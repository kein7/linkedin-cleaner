{
  "manifest_version": 3,
  "name": "LinkedIn Job Cleaner Pro",
  "version": "1.1",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs/*"],
      "js": ["content.js"]
    }
  ]
}